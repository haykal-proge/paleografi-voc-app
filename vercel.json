{
  "version": 2,
  "builds": [
    { "src": "Paleografi-voc-app/app.py", "use": "@vercel/python" },
    { "src": "build_files.sh", "use": "@vercel/static-build", "config": { "distDir": "staticfiles_build" } }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "Paleografi-voc-app/app.py" }
  ]
}